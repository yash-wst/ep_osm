doctype strict

html(xmlns='http://www.w3.org/1999/xhtml')
  head

    meta(name='robots', content='noindex, nofollow')
    meta(http-equiv='Content-Type', content='text/html; charset=UTF-8')
    meta(http-equiv='X-UA-Compatible', content='chrome=1')
    meta(name='viewport', content='width=device-width, initial-scale=0.9')

    title [[[page:title()]]]

    //- css
    link(rel='preconnect' href='https://fonts.gstatic.com')
    link(rel='stylesheet', href='lib/itx/priv/static/libs/lib.15.css')
    link(rel='stylesheet', href='lib/ep_osm/priv/static/css/ep_osm_eval_v2.css')

    //- fonts
    link(href='https://fonts.googleapis.com/css2?family=Inter:wght@300;400;600&display=swap' rel='stylesheet')

    //- javascript
    script(src='lib/itx/priv/static/external/fabricjs/fabric.min.js', type='text/javascript', charset='utf-8')
    script(src='lib/itx/priv/static/libs/lib.15.js', type='text/javascript', charset='utf-8')
    script(src='lib/ep_osm/priv/static/js/ep_osm_eval_v2.js', type='text/javascript', charset='utf-8')

    //- misc
    link(rel="stylesheet" type='text/css' href='[[[akit_lib:file(fixedColumnscss)]]]')

    //- keep this disabled
    script WebSocket = undefined;

    div [[[configs:get_ga_tracking_code()]]]

  body

    .container-fluid

        .row [[[helper_ui:flash_element()]]]

        .row.no-gutters.titlebar-main
            .col-sm-6.titlebar-col [[[ ep_osm_eval_v2_titlebar:company_branding() ]]]
            .col-sm-6.titlebar-col.titlebar-col-right [[[ ep_osm_eval_v2_titlebar:right_section() ]]]

        .row.no-gutters.navbar-main
            .col-sm-6.navbar-col [[[ ep_osm_eval_v2_navbar:get_navbar_left_section() ]]]
            .col-sm-6.navbar-col [[[ ep_osm_eval_v2_navbar:get_navbar_right_section() ]]]

        .row
            .col-sm-12 [[[ page:layout() ]]]



    #myModal.modal.fade(tabindex="-1", role="dialog", aria-labelledby="myModalLabel", aria-hidden="true", style="z-index: 2000;")
      .modal-dialog(role="document")
        .modal-content
          .modal-header
            button.close(type="button", data-dismiss="modal", aria-label="Close")
              span(aria-hidden="true") &#215;
          | [[[helper_ui:modal_body_element()]]]


    script [[[script]]]
